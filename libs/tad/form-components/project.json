{
    "name": "@tad/form-components",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/tad/form-components/src",
    "prefix": "brahmos",
    "tags": [],
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/angular:ng-packagr-lite",
            "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
            "options": {
                "project": "libs/tad/form-components/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "libs/tad/form-components/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "libs/tad/form-components/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/tad/form-components/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "libs/tad/form-components/**/*.ts",
                    "libs/tad/form-components/**/*.html"
                ]
            }
        }
    }
}
